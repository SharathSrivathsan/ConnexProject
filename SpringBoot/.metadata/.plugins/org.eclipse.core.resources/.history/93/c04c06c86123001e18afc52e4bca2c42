package com.connex.project.datamodel;

import com.fasterxml.jackson.annotation.JsonProperty;

public class DriverDetails
{
	@JsonProperty(value="age", required=true)
	private Integer age;

	@JsonProperty(value="experience", required=true)
	private Float experience;

	@JsonProperty(value="record", required=true)
	private Float record;

	@JsonProperty(value="claims", required=true)
	private Integer claims;

	@JsonProperty(value="car_value", required=true)
	private Double carValue;

	@JsonProperty(value="car_annual_mileage", required=true)
	private Long carAnnualMileage;

	@JsonProperty(value="insurance_history", required=true)
	private Integer insuranceHistory;

	public Integer getAge()
	{
		return age;
	}
	public void setAge(final Integer age)
	{
		this.age = age;
	}
	public Float getExperience()
	{
		return experience;
	}
	public void setExperience(final Float experience)
	{
		this.experience = experience;
	}
	public Float getRecord()
	{
		return record;
	}
	public void setRecord(final Float record)
	{
		this.record = record;
	}
	public Integer getClaims()
	{
		return claims;
	}
	public void setClaims(final Integer claims)
	{
		this.claims = claims;
	}
	public Double getCarValue()
	{
		return carValue;
	}
	public void setCarValue(final Double carValue)
	{
		this.carValue = carValue;
	}
	public Long getCarAnnualMileage()
	{
		return carAnnualMileage;
	}
	public void setCarAnnualMileage(final Long carAnnualMileage)
	{
		this.carAnnualMileage = carAnnualMileage;
	}
	public Integer getInsuranceHistory()
	{
		return insuranceHistory;
	}
	public void setInsuranceHistory(final Integer insuranceHistory)
	{
		this.insuranceHistory = insuranceHistory;
	}

}
