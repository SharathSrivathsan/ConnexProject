package com.connex.restservice;

public class Greeting {
	long id;
	String content;
	
	public Greeting(long idParam, String nameParam)
	{
		this.id = idParam;
		this.content = nameParam;
	}
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
}
